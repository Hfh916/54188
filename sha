local function setWalkSpeed(value)
   if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
   end
end

local function setJumpPower(value)
   if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
      game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
   end
end

local function setGravity(value)
   game.Workspace.Gravity = value
end

local SliderWalkSpeed = TabGeneral:CreateSlider({
   Name = "èµ°è·¯é€Ÿåº¦",
   Range = {0, 500},
   Increment = 1,
   Suffix = "é€Ÿåº¦",
   CurrentValue = 16,
   Flag = "WalkSpeed",
   Callback = setWalkSpeed
})

local SliderJumpPower = TabGeneral:CreateSlider({
   Name = "è·³è·ƒåŠ›é‡",
   Range = {0, 500},
   Increment = 1,
   Suffix = "åŠ›é‡",
   CurrentValue = 50,
   Flag = "JumpPower",
   Callback = setJumpPower
})

local SliderGravity = TabGeneral:CreateSlider({
   Name = "é‡åŠ›è®¾ç½®",
   Range = {0, 500},
   Increment = 198,
   Suffix = "é‡åŠ›",
   CurrentValue = 196,
   Flag = "Gravity",
   Callback = setGravity
})
local function setHealth(value)
   if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
      game.Players.LocalPlayer.Character.Humanoid.Health = value
   end
end

local SliderHealth = TabGeneral:CreateSlider({
   Name = "ç”Ÿå‘½å€¼è®¾ç½®",
   Range = {0, 1000},
   Increment = 1,
   Suffix = "ç”Ÿå‘½å€¼",
   CurrentValue = 100,
